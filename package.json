{
  "name": "wpsmith",
  "version": "0.1.3",
  "description": "The CLI for WordPress Wordsmiths - Laravel-like WordPress development with Playground and SQLite",
  "keywords": [
    "wordpress",
    "cli",
    "playground",
    "sqlite",
    "laravel",
    "artisan",
    "wp-cli",
    "development"
  ],
  "author": "Juan Manuel Garrido",
  "license": "MIT",
  "type": "module",
  "bin": {
    "wpsmith": "./bin/wordsmith.js",
    "ws": "./bin/wordsmith.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "templates",
