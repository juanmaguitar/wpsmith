    "table": "^6.9.0"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juanmaguitar/wpsmith"
  },
  "bugs": {
